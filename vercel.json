{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/countify/privacy-policy",
      "dest": "/public/countify/privacy-policy"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp|css|js|txt))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
